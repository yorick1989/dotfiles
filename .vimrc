set backspace=indent,eol,start
set mouse=c
set hlsearch
set autoindent
set expandtab
"set number"
retab
set tabstop=2
set shiftwidth=2
set softtabstop=2
set smartindent
set clipboard=unnamed

"Set Paste"
nmap ,p :set paste<ENTER>
nmap ,pp :set nopaste<ENTER>

"nmap <m-C-l> :q!<CR>
nmap <A-C-S-L> :wq!<CR>
imap <A-S-L> <ESC> :q!<CR>
imap <A-C-S-L> <ESC> :wq!<CR>"
